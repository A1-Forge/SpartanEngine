/*= PLAY/PAUSE BUTTONS ===================================================================================*/
#directusPlayButton
{
	min-width: 2em;
	padding: 3px;
	border-color: #464646;
	background-color: qlineargradient(spread:pad, x1:0.460636, y1:0, x2:0.467, y2:1, stop:0 rgba(120, 120, 120, 255), stop:1 rgba(79, 79, 79, 255));
	background-image: url(:/Images/play.png);
	background-repeat: no-repeat;
	background-position: center;
	border-style: outset;
	border-width: 1px;
	border-top-left-radius: 5px;
	border-bottom-left-radius: 5px;
}

#directusPlayButton:checked 
{
	min-width: 2em;
	padding: 3px;
	border-color: #464646;
	background-color: qradialgradient(spread:pad, cx:0.495, cy:0.5, radius:0.644, fx:0.5, fy:0.5, stop:0.204545 rgba(120, 120, 120, 255), stop:1 rgba(59, 59, 59, 255));
	background-image: url(:/Images/play.png);
	background-repeat: no-repeat;
	background-position: center;
	border-style: outset;
	border-width: 1px;
	border-top-left-radius: 5px;
	border-bottom-left-radius: 5px;
}

#directusPauseButton
{
	min-width: 2em;
	padding: 3px;
	border-color: #464646;
	background-color: qlineargradient(spread:pad, x1:0.460636, y1:0, x2:0.467, y2:1, stop:0 rgba(120, 120, 120, 255), stop:1 rgba(79, 79, 79, 255));
	background-image: url(:/Images/pause.png) contain;
